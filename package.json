{
  "name": "express-pug-starter",
  "version": "1.1.0",
  "description": "sbolel/express-pug-starter",
  "license": "MIT",
  "homepage": "https://sbolel.github.io/express-pug-starter",
  "repository": {
    "type": "git",
    "url": "git@github.com:sbolel/express-pug-starter.git"
  },
  "author": {
    "name": "Sinan Bolel",
    "email": "sinanbolel@gmail.com",
    "url": "https://sinanbolel.com"
  },
  "contributors": [],
  "main": "server/index.js",
  "engines": {
    "node": "~6.2.2",
    "npm": "~3.10.3"
  },
  "scripts": {
    "debug": "./node_modules/.bin/npm-run-all ./node_modules/.bin/nodemon server/index.js",
    "start": "./node_modules/.bin/npm-run-all sass start:prod",
    "start:prod": "node server/index.js",
    "sass": "./node_modules/.bin/gulp sass",
    "sass:watch": "./node_modules/.bin/gulp sass:watch",
    "lint:js": "./node_modules/.bin/eslint -c .eslintrc.yml ./**/*.js --ignore-path .eslintignore",
    "lint:scss": "scss-lint -c .scss-lint.yml ./app/**/*.scss",
    "lint:standard": "./node_modules/.bin/standard",
    "pretest": "./node_modules/.bin/npm-run-all lint:standard lint:js lint:scss",
    "test": "./node_modules/.bin/cross-env NODE_ENV=test ./node_modules/.bin/npm-run-all test:cover test:report",
    "test:cover": "./node_modules/.bin/istanbul cover node_modules/.bin/_mocha -R test/**/*",
    "test:report": "./node_modules/.bin/istanbul report --config .istanbul.yml --root .coverage/"
  },
  "dependencies": {
    "consolidate": "0.14.1",
    "express": "4.14.0",
    "path": "0.12.7",
    "pug": "2.0.0-beta6",
    "winston": "2.2.0"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "compass": "0.1.1",
    "compass-sass-mixins": "0.12.7",
    "cross-env": "3.0.0",
    "dotenv": "2.0.0",
    "eslint": "3.6.1",
    "gulp": "3.9.1",
    "gulp-banner": "0.1.3",
    "gulp-compass": "2.1.0",
    "gulp-sass": "2.3.2",
    "istanbul": "^0.4.5",
    "mocha": "3.1.0",
    "node-sass": "^3.10.0",
    "nodemon": "1.10.2",
    "npm-run-all": "3.1.0",
    "opn": "4.0.2",
    "standard": "8.2.0",
    "supertest": "2.0.0",
    "webpack": "1.13.2"
  },
  "optionalDependencies": {
    "contentful": "3.6.4",
    "firebase": "3.4.0"
  },
  "keywords": [
    "compass",
    "express",
    "generator",
    "jade",
    "mocha",
    "pug",
    "skeleton",
    "standard",
    "starter"
  ],
  "standard": {
    "env": ["mocha"]
  },
  "sasslintConfig": ".scss-lint.yml"
}
